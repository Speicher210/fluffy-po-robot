{
  "alias": "fluffy-po-robot.phar",
  "algorithm": "SHA1",
  "chmod": "0755",
  "banner": "Fluffy PO Robot",
  "compression": "GZ",
  "directories": [
    "src",
    "bin",
    "vendor"
  ],
  "extract": true,
  "git-commit": "git-commit",
  "git-version": "git-version",
  "main": "bin/fpr",
  "output": "fpr-@git-version@.phar",
  "replacements": {
    "manifest_url": "https://raw.githubusercontent.com/Speicher210/fluffy-po-robot/master/manifest.json"
  },
  "stub": true
}